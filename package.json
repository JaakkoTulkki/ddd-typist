    "react": "^16.4.1",
    "react-redux": "^5.0.7",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^23.1.0",
